local RobTab = Window:CreateTab("Robbing", 4483362458)

local Section = RobTab:CreateSection("Insta-Place C4",true)

local Button = RobTab:CreateButton({
    Name = "City Bank",
    Interact = 'Click',
    Callback = function()
        local args = {
            [1] = {
                [1] = "Place C4",
                [2] = getNil("C4B", "MeshPart")
            }
        }
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("InteractionEvent"):FireServer(unpack(args))
        
        
        function getNil(name,class) for _,v in next, getnilinstances() do if v.ClassName==class and v.Name==name then return v;end end end
        
        local args = {
            [1] = {
                [1] = "Place C4",
                [2] = getNil("C4A", "MeshPart")
            }
        }
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("InteractionEvent"):FireServer(unpack(args))
    end,
 })

 local Button = RobTab:CreateButton({
    Name = "County Bank",
    Interact = 'Click',
    Callback = function()
        local args = {
            [1] = {
                [1] = "Place C4",
                [2] = workspace:WaitForChild("World"):WaitForChild("Buildings"):WaitForChild("Commercial"):WaitForChild("CountyBank"):WaitForChild("ScriptableParts"):WaitForChild("CurrentWall"):WaitForChild("Wall"):WaitForChild("C4A")
            }
        } 
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("InteractionEvent"):FireServer(unpack(args))
        
        
        local args = {
            [1] = {
                [1] = "Place C4",
                [2] = workspace:WaitForChild("World"):WaitForChild("Buildings"):WaitForChild("Commercial"):WaitForChild("CountyBank"):WaitForChild("ScriptableParts"):WaitForChild("CurrentWall"):WaitForChild("Wall"):WaitForChild("C4B")
            }
        }
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("InteractionEvent"):FireServer(unpack(args))
    end,
 })

 local Section = RobTab:CreateSection("",true)

local B1Label = RobTab:CreateLabel("More to be added later.")

--[[
local Section = RobTab:CreateSection("Server Info",true)

local B1Label = RobTab:CreateLabel("Empty")
local B1LootLabel = RobTab:CreateLabel("Empty")
local B2Label = RobTab:CreateLabel("Empty")
local B2LootLabel = RobTab:CreateLabel("Empty")
local TcLabel = RobTab:CreateLabel("Empty")

local PlaceHold = RobTab:CreateLabel("")

local CopLabel = RobTab:CreateLabel("Empty")
local FpdLabel = RobTab:CreateLabel("Empty")
local McsoLabel = RobTab:CreateLabel("Empty")

---- Variables 

local mcsom = workspace.ScriptableParts.Civilians.CriminalBase.ScriptableParts.Screens.ActiveUnitsSign.MCSO.Screen.MainFrame.Availability.Text
local fpdm = workspace.ScriptableParts.Civilians.CriminalBase.ScriptableParts.Screens.ActiveUnitsSign.FPD.Screen.MainFrame.Availability.Text
]]

--local mcso = mcsom:gsub("[%[%]]", "")
--local fpd = fpdm:gsub("[%[%]]", "")

--[[

local Cops = fpd + mcso 
local b1 = workspace.ScriptableParts.Civilians.CriminalBase.ScriptableParts.Screens.HeistAvailabilities.CityBank.Screen.MainFrame.Availability.Text
local b2 = workspace.ScriptableParts.Civilians.CriminalBase.ScriptableParts.Screens.HeistAvailabilities.CountyBank.Screen.MainFrame.Availability.Text
local tc = workspace.ScriptableParts.Civilians.CriminalBase.ScriptableParts.Screens.HeistAvailabilities.TCStore.Screen.MainFrame.Availability.Text

local b1loot = workspace.World.Buildings.Commercial.CountyBank.ScriptableParts.CurrentModule:GetChildren()
local b1lootText = #b1loot > 0 and table.concat(b1loot, ", ") or "Unrobbed"
local b2loot = workspace.World.Buildings.Commercial.CountyBank.ScriptableParts.CurrentModule:GetChildren()
local b2lootText = #b2loot > 0 and table.concat(b2loot, ", ") or "Unrobbed"


---- Update Function

local function UpdateServerInfo()
    B1Label:Set("City Bank - ".. b1)
    B1LootLabel:Set("Loot - ".. b1lootText)

    B2Label:Set("City Bank - ".. b2)
    B2LootLabel:Set("Loot - ".. b2lootText)

    TcLabel:Set("TC - " .. tc)

    CopLabel:Set("Cops - ".. Cops)
    FpdLabel:Set("FPD - ".. fpd)
    McsoLabel:Set("MCSO - ".. mcso)
end 


local TestButton = RobTab:CreateButton({
    Name = "Test",
    Interact = 'Click',
    Callback = function()
        UpdateServerInfo()
    end,
 })

-- Rob Tab | End


-- Fun Tab | Start

local FunTab = Window:CreateTab("Fun", 4483362458)

local Section = FunTab:CreateSection("Taser",true)

local Dropdown = FunTab:CreateDropdown({
    Name = "Taser",
    Options = {"Spawn Taser", "Remove Taser"},
    CurrentOption = "Option 1" or {"Option 1","Option 3"},
    MultiSelection = false, -- 
    Flag = "TaserDropdown", 
    Callback = function(Option)
        if Option == "Spawn Taser" then 
            local args = {
                [1] = {
                    [1] = "EquipTool",
                    [2] = "Taser",
                    [3] = "Police"
                }
            }
            game:GetService("ReplicatedStorage"):WaitForChild("RemoteFunctions"):WaitForChild("BankFunction"):InvokeServer(unpack(args))
        elseif Option == "Remove Taser" then 
            local args = {
                [1] = {
                    [1] = "UnequipTool",
                    [2] = "Taser"
                }
            }
            game:GetService("ReplicatedStorage"):WaitForChild("RemoteFunctions"):WaitForChild("BankFunction"):InvokeServer(unpack(args))
        end 
    end,
 })

local Section = FunTab:CreateSection("TC Guns",true)

